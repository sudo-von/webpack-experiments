{
  "name": "webpack-experiments",
  "version": "1.0.0",
  "description": "Webpack experiments repository",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output-filename bundle.js --output-path ./dist --mode development",
    "build:dev": "webpack --config ./webpack.dev.js",
    "build:prod": "webpack --config ./webpack.prod.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.multi.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:hot-reload": "webpack serve --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel/webpack.config.js",
    "build:react": "webpack serve --config ./react/webpack.config.js",
    "build:url-loader": "webpack serve --config ./url-loader/webpack.config.js",
    "build:prepro": "webpack serve --config ./prepro/webpack.config.js",
    "build:prevent-duplication": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:prevent-duplication-fast": "webpack --config ./dll/webpack.config.js",
    "build:dynamic:dll": "webpack --config ./dynamic-import/webpack.dll.config.js",
    "build:dynamic": "webpack --config ./dynamic-import/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudo-von/webpack-experiments.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "von",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudo-von/webpack-experiments/issues"
  },
  "homepage": "https://github.com/sudo-von/webpack-experiments#readme",
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "webpack": "^5.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "less": "4.0.0",
    "less-loader": "7.2.0",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "5.0.0",
    "postcss-loader": "4.1.0",
    "postcss-nested": "5.0.3",
    "sass-loader": "10.1.0",
    "style-loader": "2.0.0",
    "styles": "0.2.1",
    "stylus": "0.54.8",
    "stylus-loader": "4.3.1",
    "url-loader": "4.1.1",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  }
}
